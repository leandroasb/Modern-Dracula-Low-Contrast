				"string.quoted.single.html",
				"meta.attribute.value.html",
				"entity.other.attribute-name.html",
				"entity.name.function",
				"support.function",
				"variable.function",
				"meta.function-call",
				"variable.other",
				"variable.other.readwrite",
				"variable.parameter",
				"variable.parameter.jsx",
				"variable.parameter.tsx",
				"entity.name.variable.parameter.jsx",
				"entity.name.variable.parameter.tsx",
				"support.class.component.jsx",
				"support.class.component.tsx",
				"entity.name.tag.jsx",
				"entity.name.tag.tsx",
				"meta.tag.jsx",
				"meta.tag.tsx",
				"meta.attribute.jsx",
				"meta.attribute.tsx",
				"entity.other.attribute-name.jsx",
				"entity.other.attribute-name.tsx",
				"meta.object.member.js",
				"meta.object.member.ts",
				"meta.object.member.jsx",
				"meta.object.member.tsx",
				"variable.other.property.js",
				"variable.other.property.ts",
				"variable.other.property.jsx",
				"variable.other.property.tsx",
				"support.class",
				"entity.name.type",
				"entity.name.type.module",
				"support.class.component",
				"entity.name.class.css",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.selector.css",
				"meta.selector.css",
				"pseudo-element.css",
				"entity.name.tag.css",
				"keyword.other.css",
				"keyword.control.css",
				"entity.other.attribute-name",
				"support.type.property.tsx",
				"support.type",
				"punctuation.definition.typeparameters",
				"punctuation.definition.type.begin",
				"punctuation.definition.type.end"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		// Estilo itálico para palavras-chave e tipos
		{
			"scope": [
				"constant.language.undefined",
				"constant.language.null",
				"constant.language.nullptr",
				"meta.type keyword.operator.expression.typeof",
				"meta.type keyword.operator.expression.keyof",
				"keyword.function",
				"keyword.operator.new",
				"storage.type.class",
				"storage.type.interface",
				"storage.type.enum",
				"storage.type.struct",
				"storage.type.typedef",
				"storage.type.var",
				"storage.modifier",
