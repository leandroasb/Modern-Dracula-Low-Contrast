	"tokenColors": [ // Cores para a sintaxe do código no editor
		// Cor para caracteres de escape como \n, \t, etc.
		{
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#db6aaa"
			}
		},
		// Remove o estilo de fonte em negrito de vários escopos para um visual mais limpo.
		{
			"name": "Remove o negrito de vários escopos",
			"scope": [
				"*",
				"markup",
				"markup.bold",
				"markup.strong",
				"string",
				"constant.other",
